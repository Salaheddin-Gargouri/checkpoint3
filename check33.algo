ALGORITHM checkpoint3
VAR
   i,j,n : INTEGER;
   
BEGIN
   n := tab.length;                                         //longeur du tableux
   FOR i FROM 0 TO n- 1 STEP 1  DO
       // Last i elements are already in place
       FOR j  FROM 0 TO n-i-1 STEP 1  DO
           IF (tab[j] > tab[j+1]) THEN
               swap(tab[j], tab[j+1])
           END_IF
       END_FOR
   END_FOR
END
END